{
  "name": "Iris",
  "version": "1.0.0",
  "description": "Fable build stuff",
  "main": "index.js",
  "scripts": {
    "babel": "babel src/Iris/Iris/Web/React --out-dir src/Iris/assets/frontend/react",
    "build-frontend-debug": "fable --msbuild Configuration=Debug",
    "watch-frontend-debug": "fable --msbuild Configuration=Debug -w --target watch",
    "build-frontend-release": "fable --msbuild Configuration=Release",
    "watch-frontend-release": "fable --msbuild Configuration=Release -w --target watch",
    "build-worker-debug": "fable --msbuild Configuration=Debug",
    "watch-worker-debug": "fable --msbuild Configuration=Debug -w --target watch",
    "build-worker-release": "fable --msbuild Configuration=Release",
    "watch-worker-release": "fable --msbuild Configuration=Release -w --target watch",
    "build-tests-debug": "fable --msbuild Configuration=Debug",
    "watch-tests-debug": "fable --msbuild Configuration=Debug -w --target watch",
    "build-tests-release": "fable --msbuild Configuration=Release",
    "watch-tests-release": "fable --msbuild Configuration=Release -w --target watch",
    "appveyor-tests": "mocha-phantomjs -p C:/ProgramData/chocolatey/bin/phantomjs.exe -R tap src/Iris/bin/Debug/Web.Tests/tests.html"
  },
  "author": "Karsten Gebbert, 2016, Nsynk GmbH",
  "license": "ISC",
  "readme": "README.org",
  "repository": "https://bitbucket.com/nsynk/iris.git",
  "devDependencies": {
    "@types/react": "^0.14.44",
    "@types/requirejs": "^2.1.28",
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "exports-loader": "^0.6.3",
    "fable-compiler": "^0.6.7",
    "imports-loader": "^0.6.5",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "core-js": "^2.4.0",
    "dom-delegator": "^13.1.0",
    "expect.js": "^0.3.1",
    "fable-core": "^0.6.4",
    "mocha": "^2.5.3",
    "radium": "^0.18.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "routie": "0.0.1",
    "virtual-dom": "^2.1.1"
  }
}
