namespace Iris.Serialization.Api;

table IrisClientFB {
     Id: string;
     Name: string;
}

enum CommandFB:ushort {
     RegisterFB = 0,
     UnReqisterFB = 1,
     PingFB = 2,
}

union ParameterFB {
     IrisClientFB
}

table ApiRequestFB {
     Command: CommandFB;
     Parameter: ParameterFB;      
}

enum StatusFB:ushort {
     Pong = 0,
     OK = 1,
     NOK = 2,
}

table ErrorFB {
     Error: string;
}

table ApiResponseFB {
     Status: StatusFB;
     Data: ErrorFB;      
}